{
  "theme": "Rich Vesper",
  "zoomed_padding": true,
  "tabs": {
    "show_close_button": "hover",
    "close_position": "right",
    "git_status": false,
    "file_icons": true,
  },
  "tab_bar": {
    "show_tab_bar_buttons": true,
    "show_nav_history_buttons": false,
    "show": true,
  },
  "disable_ai": true,
  "show_edit_predictions": false,
  "icon_theme": "Symbols Icon Theme",
  "rounded_selection": true,
  "always_treat_brackets_as_autoclosed": true,
  "edit_predictions": {
    "mode": "subtle",
  },
  "collaboration_panel": {
    "default_width": 320,
    "button": false,
  },
  "notification_panel": {
    "button": false,
  },
  "git_panel": {
    "default_width": 320,
    "button": false,
  },
  "outline_panel": {
    "default_width": 320,
    "indent_guides": {
      "show": "never",
    },
    "auto_fold_dirs": true,
    "file_icons": true,
    "button": false,
  },
  "bottom_dock_layout": "contained",
  "file_finder": {
    "file_icons": true,
  },
  "title_bar": {
    "show_menus": false,
    "show_sign_in": false,
    "show_user_picture": false,
    "show_onboarding_banner": false,
    "show_project_items": false,
    "show_branch_name": false,
    "show_branch_icon": false,
  },
  "debugger": {
    "button": false,
  },
  "search": {
    "button": true,
  },
  "diagnostics": {
    "button": false,
  },
  "terminal": {
    "scrollbar": {
      "show": "never",
    },
    "toolbar": {
      "breadcrumbs": false,
    },
    "default_width": 320.0,
    "copy_on_select": true,
    "minimum_contrast": 0.0,
    "blinking": "on",
    "font_size": 18.0,
    "dock": "bottom",
    "button": true,
    "cursor_shape": "block",
  },
  "status_bar": {
    "cursor_position_button": false,
    "active_language_button": false,
  },
  "agent_buffer_font_size": 16.0,
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-5.2",
    },
    "always_allow_tool_actions": true,
    "default_profile": "write",
    "model_parameters": [],
  },
  "features": {
    "edit_prediction_provider": "none",
  },
  "soft_wrap": "editor_width",
  "tab_size": 2,
  "toolbar": {
    "agent_review": false,
    "selections_menu": false,
    "quick_actions": false,
    "breadcrumbs": false,
  },
  "scrollbar": {
    "axes": {
      "vertical": false,
      "horizontal": false,
    },
  },
  "gutter": {
    "breakpoints": false,
    "folds": false,
    "line_numbers": true,
  },
  "show_wrap_guides": false,
  "indent_guides": {
    "enabled": false,
  },
  "project_panel": {
    "hide_hidden": false,
    "hide_root": false,
    "indent_guides": {
      "show": "never",
    },
    "scrollbar": {
      "show": "never",
    },
    "auto_fold_dirs": true,
    "auto_reveal_entries": true,
    "git_status": true,
    "folder_icons": true,
    "file_icons": true,
    "entry_spacing": "comfortable",
    "hide_gitignore": false,
    "default_width": 320.0,
    "button": false,
  },
  "cursor_shape": "block",
  "cursor_blink": true,
  "buffer_font_family": "GeistMono Nerd Font Mono",
  "restore_on_startup": "launchpad",
  "base_keymap": "VSCode",
  "ui_font_size": 18.0,
  "buffer_font_size": 18.0,
  "auto_indent": true,
  "auto_indent_on_paste": true,
  "autosave": {
    "after_delay": {
      "milliseconds": 100,
    },
  },
  "autoscroll_on_clicks": true,
}
