{
  "editor.bracketPairColorization.enabled": false,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "underline",
  "editor.detectIndentation": false,
  "editor.fontFamily": "'GeistMono NF', 'JetBrains Mono', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.guides.indentation": false,
  "editor.hover.enabled": false,
  "editor.insertSpaces": true,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "on",
  "editor.links": false,
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.overtypeCursorStyle": "line",
  "editor.parameterHints.enabled": false,
  "editor.tabSize": 2,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.wordWrap": "on",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmPasteNative": false,
  "files.autoSave": "afterDelay",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "scm.diffDecorations": "none",
  "symbols.hidesExplorerArrows": false,
  "terminal.integrated.accessibleViewPreserveCursorPosition": true,
  "terminal.integrated.allowedLinkSchemes": [
      "file",
      "http",
      "https",
      "mailto",
      "vscode",
      "vscode-insiders",
      "ms-settings"
  ],
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorStyleInactive": "none",
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.fontFamily": "'GeistMono NF', 'JetBrains Mono', monospace",
  "terminal.integrated.fontSize": 16,
  "window.commandCenter": false,
  "window.menuBarVisibility": "compact",
  "window.newWindowProfile": "Default",
  "window.title": " ",
  "window.zoomLevel": 1,
  "workbench.activityBar.location": "bottom",
  "workbench.colorTheme": "Monochai Dark",
  "workbench.iconTheme": "ayu-mirage-zed",
  "workbench.panel.showLabels": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.statusBar.visible": false,
  "workbench.tree.renderIndentGuides": "none",
  "cursor.cpp.disabledLanguages": [
      "markdown",
      "scminput"
  ],
  "chat.editing.alwaysSaveWithGeneratedChanges": true,
  "github.copilot.nextEditSuggestions.enabled": true,
  "custom-ui-style.font.sansSerif": "'Funnel Sans', 'Geist', sans-serif"
}